{
  "kind": "collectionType",
  "collectionName": "worksheet_images",
  "info": {
    "singularName": "worksheet-image",
    "pluralName": "worksheet-images",
    "displayName": "Worksheet Image",
    "description": "Images for worksheet generators"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "fileName": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "imageUrl": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "theme": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::image-theme.image-theme",
      "inversedBy": "images"
    },
    "tags": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "isActive": {
      "type": "boolean",
      "default": true
    }
  }
}
