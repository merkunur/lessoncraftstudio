{
  "kind": "collectionType",
  "collectionName": "image_assets",
  "info": {
    "singularName": "image-asset",
    "pluralName": "image-assets",
    "displayName": "Image Asset",
    "description": "Individual images for worksheet generators"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "fileName": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "displayName": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "translations": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "file": {
      "type": "media",
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "themes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::image-theme.image-theme"
    },
    "isPremium": {
      "type": "boolean",
      "default": false
    },
    "metadata": {
      "type": "json"
    }
  }
}