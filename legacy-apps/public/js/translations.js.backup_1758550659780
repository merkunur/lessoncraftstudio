    "this_row_has_no_content_to_crop": "[NEEDS_TRANSLATION] This row has no content to crop.",
    "no_overflow_to_crop": "[NEEDS_TRANSLATION] No overflow to crop.",
    "row_cropped_successfully": "[NEEDS_TRANSLATION] Row cropped successfully.",
    "are_you_sure_you_want_to_clear_the_worksheet_this_": "[NEEDS_TRANSLATION] Are you sure you want to clear the worksheet? This cannot be undone."
  }
};

// Helper function to get translation
function getTranslation(key, locale = 'en') {
    const localeTranslations = translations[locale] || translations['en'];
    return localeTranslations[key] || key;
}

// Helper function to translate element
function translateElement(element, key, locale = 'en') {
    const translation = getTranslation(key, locale);
    if (element.tagName === 'INPUT' || element.tagName === 'TEXTAREA') {
        element.placeholder = translation;
    } else if (element.tagName === 'OPTION' || element.tagName === 'SELECT') {
        element.value = translation;
    } else {
        element.textContent = translation;
    }
}

// Export for use in other scripts
if (typeof module !== 'undefined' && module.exports) {
    module.exports = translations;
}
